<h1>Listing Files</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Url</th>
    <th>Expriy date</th>
    <th></th>
  </tr>

<% @filedetail.each do |fd| %>
  <tr>
    <td><%= fd.fdata.original_filename %></td>
    <td><%= link_to "#{fd.fdata.url}", autheticate_filedetails_path(:id => fd.id) %></td>
     <td><%= fd.fdata_days %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New File', new_filedetail_path %>
