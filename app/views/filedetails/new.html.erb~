<% form_for @filedetail, :html => { :multipart => true } do |f| %>
  <% if @filedetail.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@filedetails.errors.count, "error") %> prohibited this developer from being saved:</h2>

      <ul>
      <% @filedetail.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<div>
	<%= f.label :file %>
	<%= f.file_field :fdata %>
</div>
<div>
	<%= f.label :password %>
	<%= f.text_field :fdata_password %>
</div>
<div>
	<%= f.label :days %>
	<%= f.text_field :fdata_days %>
</div>

<div>
	<%= f.submit %>
</div>

<% end %>
